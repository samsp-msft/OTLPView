<MudDialog>
    <DialogContent>
        <MudTable Items="@Properties" Hover="true" Class="pa-0 ma-0">
            <HeaderContent>
                <MudTh>Key</MudTh>
                <MudTh>Value</MudTh>
            </HeaderContent>
            <RowTemplate>
                <MudTd DataLabel="Key">
                    <strong>@context.Key</strong>
                </MudTd>
                <MudTd DataLabel="Value">
                    <code class="pa-1">@context.Value</code>
                </MudTd>
            </RowTemplate>
        </MudTable>
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Primary" Size="Size.Large"
                   Variant="Variant.Filled"
                   StartIcon="@Icons.Material.Filled.Check"
                   OnClick="@Ok" Class="mr-4 mb-4">
            Ok
        </MudButton>
    </DialogActions>
</MudDialog>
